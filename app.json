{
  "expo": {
    "name": "Sheet Music For Kids",
    "slug": "SheetMusicForKids",
    "version": "1.0",
    "userInterfaceStyle": "light",
    "platforms": [
      "ios",
      "android"
    ],
    "privacy": "public",
    "description": "A colorful and fun app that helps young children learn sheet music.",
    "owner": "musicaldev",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/SplashScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.leamcshan.sheetmusickids",
      "infoPlist": {
        "UISupportedInterfaceOrientations~ipad": [
          "UIInterfaceOrientationLandscapeLeft",
          "UIInterfaceOrientationLandscapeRight"
        ]
      },
      "requireFullScreen": true
    }
  },
  "android": {
    "package": "com.musicaldev.SheetMusicForKids",
    "permissions": []
  },
  "extra": {
    "supportEmail": "the.holy.dev@gmail.com",
    "supportsDataCollection": false,
    "privacyPolicyUrl": "https://docs.google.com/document/d/1V-YAeX8ZNRgB_VHe1pI19mk4YA5uqCjUH8oRMHAHti0/edit?usp=sharing",
    "eas": {
      "projectId": "543f8cb4-3b36-4d2a-9888-e0fb45b94cec"
    }
  },
  "assetBundlePatterns": [
    "**/*"
  ],
  "web": {},
  "updates": {
    "fallbackToCacheTimeout": 0
  },
  "eas": {
    "projectId": "543f8cb4-3b36-4d2a-9888-e0fb45b94cec"
  }
}